{
  "imports": {
    "std/": "https://deno.land/std@0.205.0/"
  },
  "tasks": {
    "run": "deno run -A --check main.ts --run-tasks",
    "log-course-tasks": "deno run -A main.ts --log-course-tasks",
    "log-enrolment-tasks": "deno run -A main.ts --log-enrolment-tasks",
    "aliases": "deno run -A --check main.ts --view-course-aliases",
    "view-subject": "deno run -A --check main.ts --view-subject",
    "view-composites": "deno run -A --check main.ts --view-composites",
    "edit-course-member": "deno run -A --check main.ts --COURSE-MEMBER",
    "delete-course": "deno run -A --check main.ts --delete-course",
    "transfer-ownership": "deno run -A --check main.ts --transfer-course-ownership",
    "list-courses": "deno run -A --check main.ts --list-courses",
    "dailyorg": "deno run -A --check main.ts --dailyorg-replacements",
    "staging": "deno run -A --check main.ts --staging",
    "scratch": "deno run -A --check main.ts --scratch"
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true,
    "include": ["**"],
    "lineWidth": 90
  }
}
